{
  "config": {
    "data": {
      "admin": {
        "role": {
          "publish": "*@adobe.com"
        }
      }
    }
  },
  "metadata": {
    "data": {
      "/*": [
        {
          "key": "Theme",
          "value": "red"
        },
      ]
    }
  },
  "headers": {
    "data": {
      "/*": [
        { 
          "key": "content-security-policy",
          "value": "default-src https:"
        }
      ]
    }
  }
}
